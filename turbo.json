{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", "storybook-static/**", ".next/**"],
      "dependsOn": ["^build"]
    },
    "//#check": {},
    "//#fix": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "clean": {
      "cache": false
    },
    "preview-storybook": {
      "dependsOn": ["^build"],
      "cache": false
    }
  }
}
