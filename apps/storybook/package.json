    "country-data-list": "^1.6.0",
    "date-fns": "^4.1.0",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-wheel-gestures": "^8.1.0",
    "libphonenumber-js": "^1.12.37",
    "lucide-react": "^0.575.0",
    "react": "^19.2.4",
    "react-circle-flags": "^0.0.27",
    "react-day-picker": "^9.13.2",
    "react-dom": "^19.2.4",
    "react-icons": "^5.5.0",
